{
  "name": "questionnaire-schneider",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "docker:build": "docker build -t questionnaire-schneider .",
    "docker:run": "docker run -p 3000:3000 questionnaire-schneider",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up -d --build",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "deploy:build": "npm run type-check && npm run lint && npm run build",
    "deploy:docker": "npm run deploy:build && npm run docker:build",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:all": "rm -rf .next out node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@types/multer": "^1.4.11",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "lucide-react": "^0.294.0",
    "multer": "^1.4.5-lts.1",
    "next": "^14.2.33",
    "openai": "^6.5.0",
    "postcss": "^8",
    "qrcode": "^1.5.4",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.0.4"
  }
}
